{
  "name": "@impervaos/google-analytics-spa",
  "version": "1.6.2",
  "description": "Automatic Google Analytics reporting for your single page application, used with Redux in React, Angular, Vue",
  "keywords": [
    "google",
    "analytics",
    "React",
    "redux",
    "middleware",
    "ReactJS",
    "GA",
    "javascript",
    "Google Analytics",
    "Angular",
    "Vue",
    "mobx",
    "single",
    "page",
    "application",
    "report",
    "user",
    "behavior",
    "Universal Analytics"
  ],
  "main": "lib/google-analytics-spa.min.js",
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "start": "webpack-dev-server --env.development --inline --hot --progress --colors --config webpack.dev.config.js",
    "dev": "yarn start",
    "build": "webpack --env.production --progress  --config webpack.prod.config.js",
    "build:ci": "yarn install && yarn test && yarn build && yarn badges",
    "build:maps": "yarn build -- --env.withMaps",
    "build:analyze": "yarn build -- --env.analyze --json > build.stats.json",
    "test": "jest --coverage --config jest.json",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "badges": "jest-coverage-badges --input 'build/test-coverage/ui/coverage-summary.json' --output 'badges'",
    "docs": "node docs-generator.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/imperva/google-analytics-spa.git"
  },
  "author": "Michael Rodov",
  "license": "MIT",
  "peerDependencies": {
    "history": "^4.10.1"
  },
  "devDependencies": {
    "@babel/core": "^7.11.4",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-decorators": "^7.10.5",
    "@babel/plugin-transform-modules-commonjs": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.11.0",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-react": "^7.10.4",
    "@types/jest": "^24.9.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-polyfill": "^6.26.0",
    "clean-webpack-plugin": "^3.0.0",
    "core-js": "2.5.7",
    "css-loader": "^3.4.2",
    "enzyme": "3.4.4",
    "enzyme-adapter-react-16": "1.2.0",
    "eslint": "^7.7.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.1.0",
    "fibers": "^5.0.0",
    "history": "^4.10.1",
    "jest": "^24.9.0",
    "jest-cli": "^25.5.4",
    "jest-coverage-badges": "^1.1.2",
    "jest-css-modules": "^2.1.0",
    "jest-junit": "^10.0.0",
    "markdown-magic": "^1.0.0",
    "markdown-magic-jsdoc": "^1.0.1",
    "node-sass": "^7.0.0",
    "peer-deps-externals-webpack-plugin": "^1.0.4",
    "pre-commit": "^1.2.2",
    "prismjs": "^1.20.0",
    "prop-types": "^15.7.2",
    "random-name": "^0.1.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-test-renderer": "^16.4.2",
    "redux": "^4.0.5",
    "regenerator-runtime": "^0.13.3",
    "sass": "^1.26.5",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.2",
    "svg-url-loader": "^3.0.3",
    "terser-webpack-plugin": "^2.3.5",
    "url-loader": "^3.0.0",
    "webpack": "^4.41.5",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^4.2.2",
    "webpack-monitor": "^1.0.14",
    "whatwg-fetch": "^3.0.0"
  },
  "dependencies": {
    "is-empty": "^1.2.0"
  },
  "pre-commit": [
    "build",
    "test"
  ]
}
