    "react-syntax-highlighter": "^11.0.2",
    "react-test-renderer": "^16.4.2",
    "regenerator-runtime": "^0.13.3",
    "sass": "^1.26.5",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.2",
    "svg-url-loader": "^3.0.3",
    "terser-webpack-plugin": "^2.3.5",
    "url-loader": "^3.0.0",
    "webpack": "^4.41.5",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-cli": "^3.3.10",
